{
	"name": "typescheme",
	"version": "0.0.1",
	"description": "TypeScheme is a LISP-like that compiles to TypeScript types",
	"main": "index.js",
	"author": "Terran One",
	"license": "MIT",
	"devDependencies": {
		"@types/antlr4": "^4.11.2",
		"@types/lodash": "^4.14.191",
		"@types/node": "^18.11.11",
		"antlr4ts": "^0.5.0-alpha.4",
		"antlr4ts-cli": "^0.5.0-alpha.4",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.3"
	},
	"scripts": {
		"build": "tsc --module commonjs && webpack --mode production",
		"build:types": "tsc --declaration --emitDeclarationOnly",
		"antlr-lexer": "antlr4ts ./grammar/TypeSchemeLexer.g4 -o ./src",
		"antlr-parser": "antlr4ts -visitor ./grammar/TypeSchemeParser.g4 -o ./src -lib ./src/grammar",
		"antlr": "npm run antlr-lexer && npm run antlr-parser",
		"start": "ts-node ./test.ts"
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"dependencies": {
		"antlr4": "^4.11.0",
		"lodash": "^4.17.21",
		"reflect-metadata": "^0.1.13",
		"runtypes": "^6.6.0",
		"ts-morph": "^17.0.1"
	}
}
