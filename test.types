(define (Prefix P T)
  [P, ...T]
)

(define (Lens T P)
  (if (extends? P (Prefix any any[]))
  	(if (extends? (First P) (keyof T))
  		(if (extends? (Shift P) (Prefix any any[]))
  			(Lens T[First<P>] (Shift P))
  			T[(First P)]
      )
    )
  T)
)

(define Obj {a: number, b: string})
(define A (Lens T ['a', 'b']))